@using SampleProject.Areas.Admin.ViewModels.AdminSetup
@model List<IndexViewModel>
@{
    ViewBag.Title = "AdminSetup";
}

<h2>AdminSetup</h2>

No admin exists.

Choose a user from list to promote him to admin.

<table>
    <tr>
        <th>
            UserId
        </th>
        <th>
            Username
        </th>
        <th>
            OpenId
        </th>
        <th></th>
    </tr>

@foreach (var user in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelUser => user.UserId)
        </td>
        <td>
            @Html.DisplayFor(modelUser => user.Username)
        </td>
        <td>
            @Html.DisplayFor(modelUser => user.OpenId)
        </td>
        <td>
            @Html.ActionLink("Promote", "Promote", new { userId = user.UserId })
        </td>
    </tr>
}

</table>







